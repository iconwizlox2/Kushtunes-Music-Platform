{
  "version": 2,
  "name": "kushtunes-mobile-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/site.webmanifest",
      "headers": {
        "Content-Type": "application/manifest+json"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}


