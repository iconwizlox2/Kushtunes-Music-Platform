# S3-compatible storage (works with AWS S3, Backblaze B2, Wasabi, etc.)
S3_ENDPOINT=https://s3.your-provider.com
S3_REGION=auto
S3_BUCKET=kushtunes-uploads
S3_ACCESS_KEY_ID=xxxxxxxx
S3_SECRET_ACCESS_KEY=xxxxxxxx

# NextAuth (or your auth of choice; optional for now)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-long-random-secret

# Postgres (Supabase/Neon/Railway)
DATABASE_URL="postgresql://user:pass@host:5432/kushtunes"
