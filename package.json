{
  "name": "kushtunes-platform",
  "version": "2.0.0",
  "description": "Professional music distribution platform - From the Nile to the World",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_DISABLE_BUILD_TRACE=1 NEXT_TELEMETRY_DISABLED=1 NEXT_PRIVATE_DISABLE_TRACE=1 NEXT_PRIVATE_DISABLE_TELEMETRY=1 NODE_OPTIONS='--max-old-space-size=8192' prisma generate && NEXT_DISABLE_BUILD_TRACE=1 NEXT_TELEMETRY_DISABLED=1 NEXT_PRIVATE_DISABLE_TRACE=1 NEXT_PRIVATE_DISABLE_TELEMETRY=1 next build --no-lint",
    "build:prod": "NODE_OPTIONS='--max-old-space-size=8192' prisma generate && next build",
    "start": "node .next/standalone/server.js",
    "start:next": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "setup-db": "prisma generate && prisma db push",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "deploy": "./scripts/deploy-production.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=out",
    "postinstall": "prisma generate && ./create-trace-files.sh"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.0",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.0",
    "prisma": "^6.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "vercel": "^48.1.6"
  },
  "keywords": [
    "music",
    "mobile",
    "pwa",
    "nextjs",
    "react",
    "tailwindcss",
    "music-distribution",
    "africa"
  ],
  "author": "Kushtunes Team",
  "license": "MIT"
}
