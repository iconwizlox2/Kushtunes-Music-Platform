{
  "name": "kushtunes-mobile-app",
  "version": "1.0.0",
  "description": "Mobile-first music release platform - From the Nile to the World",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' prisma generate && npx next build --no-lint",
    "build:minimal": "NODE_OPTIONS='--max-old-space-size=4096' prisma generate && NEXT_DISABLE_BUILD_TRACE=1 next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "deploy": "./deploy.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=out",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.0",
    "postcss": "^8.4.0",
    "prisma": "^6.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0"
  },
  "keywords": [
    "music",
    "mobile",
    "pwa",
    "nextjs",
    "react",
    "tailwindcss",
    "music-distribution",
    "africa"
  ],
  "author": "Kushtunes Team",
  "license": "MIT"
}
